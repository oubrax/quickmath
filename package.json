{
  "name": "quickmath",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on tcp:5173 && cross-env ELECTRON_START_URL=http://localhost:5173 electron .\"",
    "build": "vite build",
    "start": "electron .",
    "preview": "vite preview",
    "dist:linux": "npm run build && electron-builder --linux",
    "dist:mac": "npm run build && electron-builder --mac zip",
    "dist:mac:dmg": "npm run build && electron-builder --mac dmg",
    "dist:win:dir": "npm run build && electron-builder --win --dir",
    "dist:win:zip": "npm run build && electron-builder --win zip"
  },
  "build": {
    "appId": "com.quickmath.app",
    "productName": "QuickMath",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": [
        "zip"
      ]
    }
  },
  "dependencies": {
    "@cortex-js/compute-engine": "^0.30.2",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "install": "^0.13.0",
    "lucide-react": "^0.561.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-mathlive": "^3.0.5-preview.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^31.7.7",
    "electron-builder": "^26.0.12",
    "rollup": "^4.53.5",
    "tailwindcss": "^4.0.0",
    "tw-animate-css": "^1.4.0",
    "vite": "^5.4.11",
    "wait-on": "^8.0.1"
  }
}
